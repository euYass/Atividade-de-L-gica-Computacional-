programa {
  funcao inicio() {
  inteiro multiplicando = 6 , multiplicador = 0, resultado 

  escreva("tabuada\n\n")

  escreva("digite o multiplicando: ")
  leia(multiplicando)

  para(multiplicador = 0; multiplicador <=10; multiplicador++){
        resultado = multiplicando*multiplicador
        escreva(multiplicando, " X ",multiplicador," = ",resultado," \n")
    }



  }
}
